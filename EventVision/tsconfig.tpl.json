{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "ES2020",
    "skipLibCheck": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019",
      "ES2020.BigInt"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiEventGrid/TcHmiEventGrid.d.ts"
  ],
  "files": [
    "EventVisionControl/EventVisionControl.ts",
    "Scripts/typings/jquery/jquery.d.ts",
    "Scripts/typings/jqueryui/jqueryui.d.ts"
  ]
}